{
  "name": "xutl/yii2-fontawesome",
  "description": "The fontawesome extension for the Yii framework",
  "type": "yii2-extension",
  "license": "BSD-3-Clause",
  "keywords": [
    "fontawesome",
    "yii2"
  ],
  "authors": [
    {
      "name": "XuTL",
      "email": "xutl@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.4.0",
    "yiisoft/yii2": "*",
    "bower-asset/fontawesome": "^4.5"
  },
  "autoload": {
    "psr-4": {
